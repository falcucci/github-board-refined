{
  "manifest_version": 2,
  "name": "react-es6-popup-example",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/projects/*"
      ]
    }
  ],
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "48": "extension/images/final-state-48.png"
    },
    "default_title": "Github Board Refined",
    "default_popup": "extension/popup.html"
  },

  "permissions": ["activeTab"]
}
